#=# Aliases
aliases=doskey /macros
aliashelp=(echo F7 - display command history && echo.Alt+F7 - clear command history && echo.[chars]F8 - search for command && echo.F9 - select command by number && echo.Alt+F10 - clear macro definitions)

#=# Git
g=git $*
gs=git status
ga=git add -A$tgit status
gc=git commit -m$*
gps=git push
gpsh=git push
gpl=git pull
gl=git log $*
gln=git log -n$*
gr=git reset $*

#=# Navigation
js=pushd \work\sevendigital-com\src\SevenDigital.Com.Web\Static\js
wk=pushd \work
~=pushd %USERPROFILE%
`=pushd ..\$*

#=# Apps
sln=for %f in (*.sln) do start %f
.=explorer .
sub="c:\Program Files\Sublime Text 2\sublime_text.exe" $*